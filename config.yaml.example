    name: "central worker service"

postgres:
  host: ""
  db: ""
  use"
  port: "5432"
  ssl_mode: "disable"

redis:
  addr: ""
  password: ""
  db: 5
  min: 1
  max: 5
  worker_broker_host: "redis://mm/2"