server:
  env: "development"
  port: "8080"
  log:
    level: "INFO"
    sentry:
      dsn: ""

worker:
  concurrency: 5
  task:
    mailing:
      max_retry: 10,
      timeout_seconds: 10
    mail_updating:
      max_retry: 5
      timeout_seconds: 10


sendinblue:
  api_key: ""
